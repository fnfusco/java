package entities;

public class Matriz {
	
	public Matriz() {
	}
	
	public static void desenhaMatriz() {
		int[][] matriz = new int[8][8];
		int l,c;
		
		for(l=0;l<matriz.length;l++) {
			for(c=0; c<matriz.length; c++) {
				matriz[l][c] = 0;
				System.out.print(matriz[l][c] + " ");
			}		
			System.out.println();
		}
		
		System.out.println();
		System.out.println();
		
		for(l=0;l<matriz.length;l++) {
			for(c=0; c<matriz.length; c++) {
				
			}
			System.out.println();
		}
		
		for(l=0;l<matriz.length;l++) {
			for(c=0; c<matriz.length; c++) {
				System.out.print(matriz[l][c] + " ");
			}
			System.out.println();
		}
				
	}
	
}


